			<class name="com.dupr.test.DUPRLoginTest" />
			<class name="com.dupr.test.NegativeLoginTest" />
		</classes>
	</test>

	<test name="Signup Test">
		<classes>
			<class name="com.dupr.test.signup.SignupTest" />
			<class name="com.dupr.test.signup.DUPRClaimAccountSignUpTest" />
		</classes>
	</test>

	<test name="Forgot Password">
		<classes>
