<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Common UI Automation Tests">

	<test name="Login Test">
		<classes>
			<class name="com.dupr.test.DUPRLoginTest" />
			<class name="com.dupr.test.NegativeLoginTest" />
		</classes>
	</test>

	<test name="Signup Test">
		<classes>
			<class name="com.dupr.test.signup.SignupTest" />
			<class name="com.dupr.test.signup.DUPRClaimAccountSignUpTest" />
		</classes>
	</test>

	<test name="Forgot Password">
		<classes>
			<class name="com.dupr.test.ForgotPasswordTest" />
		</classes>
	</test>

	<test name="Add A Match">
		<classes>
			<class name="com.dupr.test.addamatch.AddAMatchTest" />
		</classes>
	</test>

	<test name="Home Menu">
		<classes>
			<class
				name="com.dupr.test.home.ValidateStatsChangeOnNewMatchTest" />
			<class name="com.dupr.test.home.ValidateRecentlyAddedMatchTest" />
			<class name="com.dupr.test.home.DeleteRecentlyAddedMatchTest" />
		</classes>
	</test>

	<test name="Search Players Menu">
		<classes>
			<class name="com.dupr.test.players.SearchPlayersTest" />
			<class name="com.dupr.test.players.BrowsePlayersTest" />
		</classes>
	</test>

	<test name="Profile Menu">
		<classes>
			<class name="com.dupr.test.profile.NotificationsTest" />
			<class name="com.dupr.test.profile.PlayerProfileTest" />
			<class name="com.dupr.test.profile.ProfilePictureTest" />
			<class name="com.dupr.test.profile.ChangePasswordTest" />
		</classes>
	</test>
</suite>